{
  "kind": "collectionType",
  "collectionName": "properties",
  "info": {
    "singularName": "property",
    "pluralName": "properties",
    "displayName": "Property"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Description": {
      "type": "text",
      "required": true
    },
    "Price": {
      "type": "integer",
      "required": true
    },
    "Property_Type": {
      "type": "enumeration",
      "enum": [
        "Apartment",
        "Townhouse",
        "Villa ",
        "Land ",
        "Comercial"
      ]
    },
    "Latitude": {
      "type": "string",
      "required": true
    },
    "Longitude": {
      "type": "string",
      "required": true
    },
    "Bedrooms": {
      "type": "integer"
    },
    "Current_Status": {
      "type": "enumeration",
      "required": true,
      "default": "Active",
      "enum": [
        "Active",
        "Draft",
        "Sold ",
        "Rented"
      ]
    },
    "Bathrooms": {
      "type": "integer"
    },
    "Area": {
      "type": "integer"
    },
    "Images": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Listing_Type": {
      "type": "enumeration",
      "required": true,
      "default": "For Sale",
      "enum": [
        "For Sale",
        "For Rent",
        "Sold"
      ]
    },
    "Slug": {
      "type": "uid",
      "targetField": "Title",
      "required": true
    }
  }
}
